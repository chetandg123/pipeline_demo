pipeline {
    agent any

    stages {
        stage('Staging') {
            steps {
                echo 'Staging Code Deployed '
            }
        }
        stage('Dev') {
            steps {
                echo 'Dev Code Deployed '
            }
        }
        stage('QA') {
            steps {
                echo 'QA Code Deployed '
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deployed Code '
            }
        }
    }
}
